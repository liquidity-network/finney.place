{
  "name": "nocust-client",
  "private": false,
  "version": "2.1.2",
  "description": "Client library to interact with NOCUST Commit-chains",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": true,
  "files": [
    "dist",
    "webpack.config.js"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build-minified": "npm run build && uglifyjs-folder dist --each --output dist --extension .js && find ./dist -type f -name '*.map' -delete",
    "release": "npm run build-minified && curl https://raw.githubusercontent.com/guifel/nocust-client-doc/master/README.md > README.md && npm publish",
    "coverage": "nyc npm test",
    "fix-lint": "eslint --fix webpack.config.js lib/*.ts lib/**/*.ts lib/**/**/*.ts ",
    "lint": "eslint lib/**/*.ts",
    "build-bundle": "rm -rf dist && webpack --mode=production --display-error-details",
    "test": "mocha -r ts-node/register -r source-map-support/register --timeout 600000 --exit",
    "build-api-ref": "typedoc --theme markdown --mdHideSources --mdEngine gitbook --out ./temp --includes ./lib/interfaces/nocust-manager.interface.ts && mkdir doc && mv ./temp/interfaces/_interfaces_nocust_manager_interface_.nocustmanagerinterface.md ./doc/client-api.md && rm -rf ./temp && sed -i 1,45d ./doc/client-api.md"
  },
  "keywords": [
    "off-chain",
    "blockchain",
    "ethereum",
    "scalability",
    "nocust",
    "liquidity",
    "network",
    "plasma",
    "payment",
    "commit-chain"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guifel/nocust-client-doc"
  },
  "contributors": [
    "Rami Khalil <rami@liquidity.network>",
    "Guillaume Felley <guillaume.felley@liquidity.network>",
    "George Sedky <george.sedky@liquidity.network>"
  ],
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/guifel/nocust-client-doc/issues",
    "homepage": "https://docs.liquidity.network"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.ts": [
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": [
      "dist/**"
    ]
  },
  "browser": {
    "request": "xhr"
  },
  "dependencies": {
    "@types/node": "^9.6.39",
    "@types/request": "^2.48.1",
    "@types/web3": "^1.0.14",
    "@types/ws": "^6.0.1",
    "async-mutex": "^0.1.3",
    "bignumber.js": "8.x",
    "ethereumjs-util": "^6.1.0",
    "hdkey": "^1.1.1",
    "https-proxy-agent": "^2.2.1",
    "inversify": "^4.13.0",
    "isomorphic-ws": "^4.0.1",
    "json-bigint": "^0.2.3",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "sha3": "^1.2.2",
    "web3-utils": "^1.0.0-beta.37",
    "ws": "^6.1.2",
    "xhr": "^2.5.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.5",
    "bignumber.js": "^7.2.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dts-bundle-webpack": "^1.0.2",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-typescript": "^0.13.0",
    "husky": "^1.1.4",
    "lint-staged": "^8.0.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.1.0",
    "prettier": "^1.15.2",
    "source-map-support": "^0.5.9",
    "ts-loader": "^4.5.0",
    "ts-node": "^6.2.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^1.2.1",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "^21.0.1",
    "uglifyjs-folder": "^1.5.1",
    "web3": "1.0.0-beta.36",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  },
  "peerDependencies": {
    "web3": "1.0.0-beta.36",
    "bn.js": "*"
  }
}
